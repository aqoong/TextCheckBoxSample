<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextCheckBox">
        <!-- TextColor is reversal colors -->
        <attr name="defaultColor" format="color"/>
        <attr name="checkedColor" format="color"/>
        <attr name="mode" format="enum">
            <enum name="text" value="0"/>
            <enum name="image" value="1"/>
        </attr>

    </declare-styleable>
</resources>